<!-- two different nav bars -->
<% if !teacher_signed_in? && !student_signed_in? && !administrator_signed_in?%>
  <%= render '/layouts/nav_guest' %>
<% elsif student_signed_in? %>
  <%= render '/layouts/nav_student' %>
<% elsif teacher_signed_in? %>
  <%= render '/layouts/nav_teacher' %>
<%else%>
  <%= render '/layouts/nav_administrator' %>

<% end %>